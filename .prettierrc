{
    "trailingComma": "es5",
    "useTabs": false,
    "bracketSameLine": true,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "printWidth": 120,
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "tabWidth": 4,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
